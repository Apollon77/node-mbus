{
  "name": "node-mbus",
  "version": "0.0.1",
  "description": "libmbus binding",
  "keywords": [
    "mbus"
  ],
  "main": "build/Release/mbus",
  "engines": {
    "node": ">= 4"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "prepublish":"rm -rf libmbus && git clone https://github.com/Apollon77/libmbus.git libmbus && cp config.h libmbus/config.h",
    "build": "node-gyp build",
    "install": "node-gyp rebuild",
    "test": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec"
  },
  "author": {
    "name": "Semen Krivosheev",
    "email": "semen.krivosheev@gmail.com"
  },
  "maintainers": [
    {
      "name": "Semen Krivosheev",
      "email": "semen.krivosheev@gmail.com"
    },
    {
      "name": "Apollon77",
      "email": "ingo@fischer-ka.de"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Apollon77/node_mbus"
  },
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.3.0",
    "nan": "~2.8.0"
  },
  "devDependencies": {
    "mocha": "^5.0.0",
    "chai": "^4.1.2",
    "istanbul": "*"
  }
}
